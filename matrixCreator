#include <fstream>
#include <iostream>
#include <sstream>
#include <ctime>


using namespace std;

int main()
{
    int num;
    int val = 1;
    int uval = 0;
    ofstream file;
    srand(time(NULL));
    file.open("C:/Users/Goose/source/repos/newG.txt", ios::app);
    for (int i = 0; i < 30; i++) {
        for (int j = 0; j < 30; j++) {
            stringstream sstr;
            num = rand();
            if (num % 2 == 0) {
                sstr << 1;
            }
            else {
                sstr << 0;
            }
            if (j != 29) {
                sstr << ", ";
            }
            file << sstr.str();
        }
        file << "\n";
    }
}
